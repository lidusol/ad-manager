import { LyOverlayRef } from '@alyle/ui';
export declare class LyDialogRef {
    private _overlayRef;
    private _result;
    get afterOpened(): import("rxjs").Observable<void>;
    get beforeClosed(): import("rxjs").Observable<any>;
    get afterClosed(): import("rxjs").Observable<any>;
    /**
     * @internal
     * @docs-private
     */
    get result(): any;
    constructor(_overlayRef: LyOverlayRef);
    close(result?: any): void;
}
