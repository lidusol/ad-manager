import { LyTheme2 } from '../theme/theme2.service';
import { Platform } from '@angular/cdk/platform';
export declare class LyOverlayContainer {
    private theme;
    private _platform;
    private _classes;
    protected readonly _containerElement: HTMLElement;
    private _items;
    get overlayLen(): number;
    private _isActiveOverlayContainer;
    constructor(theme: LyTheme2, _platform: Platform);
    get containerElement(): HTMLElement;
    /**
     * Add instance
     * @ignore
     */
    _add(item: Element, elementAfter?: Element): void;
    /**
   * Remove instance
   * @ignore
   */
    _remove(item: any): void;
    /**
     * Update styles for overlay container
     * @ignore
     */
    private _update;
}
